			   	        			"<th>课程号</th><th></th><th>课程名称</th><th>学时</th><th>课程性质</th><th>学分</th><th>课程类别</th><th>最大成绩</th><th>成绩绩点</th><th>成绩</th>"+
			   	        			"<th>重修成绩1</th><th>重修成绩2</th><th>建议修读学年</th><th>学期</th></tr></thead>"+
				    				"<tbody id='tbodycxcyqkxfyq'></tbody></table>").appendTo($("#kcxxcxcyqkxfyq"));
		    			}else{
		    				$("<p class='pull-left'></p><table class='table text-center'><thead>"+
			    					"<tr><th>修读状态</th><th>成绩学年</th><th>学期</th>"+
			   	        			"<th>课程号</th><th></th><th>课程名称</th><th>学时</th><th>课程性质</th><th>学分</th><th>课程类别</th><th>最大成绩</th><th>成绩绩点</th><th>成绩</th>"+
			   	        			"<th>补考</th><th>重修</th><th>建议修读学年</th><th>学期</th></tr></thead>"+
				    				"<tbody id='tbodycxcyqkxfyq'></tbody></table>").appendTo($("#kcxxcxcyqkxfyq"));
		    			}
	    			}
	    			else {
			        	var name = getJdsxName('1');
		        		$("<a href='javascript:;' class='more'>"+
		        		"共（<font name='zms'>0</font>）门  通过（<font name='yxmc'>0</font>）门 "+
	    	        	"<span class='glyphicon glyphicon-chevron-down'></a>").appendTo($("#showKccxcyqkxfyq"));
						if(xxdm == '11483'){
							$("<p class='pull-left'></p><table class='table text-center'>"+"<thead>"+
			        				"<tr>"+name+": </tr>"+
			    					"<tr><th>修读状态</th><th>成绩学年</th><th>学期</th>"+
			   	        			"<th>课程号</th><th></th><th>课程名称</th><th>学时</th><th>课程性质</th><th>学分</th><th>课程类别</th><th>最大成绩</th><th>成绩绩点</th><th>成绩</th>"+
			   	        			"<th>重修成绩1</th><th>重修成绩2</th></tr></thead>"+
				    				"<tbody id='tbodycxcyqkxfyq'></tbody></table>").appendTo($("#kcxxcxcyqkxfyq"));
		    			}else{
		    				$("<p class='pull-left'></p><table class='table text-center'>"+"<thead>"+
			        				"<tr>"+name+": </tr>"+
			    					"<tr><th>修读状态</th><th>成绩学年</th><th>学期</th>"+
			   	        			"<th>课程号</th><th></th><th>课程名称</th><th>学时</th><th>课程性质</th><th>学分</th><th>课程类别</th><th>最大成绩</th><th>成绩绩点</th><th>成绩</th>"+
			   	        			"<th>补考</th><th>重修</th></tr></thead>"+
				    				"<tbody id='tbodycxcyqkxfyq'></tbody></table>").appendTo($("#kcxxcxcyqkxfyq"));
		    			}
			        }
		    	}
	    	}
   		
		loadKc();
		 loadJd();
	   	zTree();
	});
</script>
