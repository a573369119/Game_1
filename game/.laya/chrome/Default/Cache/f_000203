	<script type="text/javascript">
	var sjfwTempModel={};
	var dqzc=parseInt("14");
	var dqxq=parseInt("2");
	var cdkjysjkz=parseInt("365");//场地可借用时间控制，可借用当前日期之后n天内的场地
	var zdkxzc=dqzc+((dqxq+cdkjysjkz%7>7)?Math.ceil(cdkjysjkz/7):Math.floor(cdkjysjkz/7));//最大可选周次
	var zdkxxq=dqxq+cdkjysjkz%7>7?(dqxq+cdkjysjkz%7)%7:dqxq+cdkjysjkz%7;//最大可选星期
	//扩展自己的方法
	jQuery(function($){
		//查询符合条件的空闲场地
		function queryFunc(){
			var map = {};
			var zcList = [];
			var xqList = [];
			var jcList = [];	
			map["fwzt"] = jQuery('#fwzt').val();
			map["xqh_id"] = jQuery('#xqh_id').val();
			map["xnm"] = jQuery('#xnm').val();
			map["xqm"] = jQuery('#xqm').val();
			map["cdlb_id"] = jQuery('#cdlb_id').val();
			map["cdejlb_id"] = jQuery('#cdejlb_id').val();
			map["qszws"] = jQuery('#qszws').val();
			map["jszws"] = jQuery('#jszws').val();			
			map["cdmc"] = jQuery('#cdmc').val();
			map["lh"] = jQuery('#lh').val();
			map["qssd"] = jQuery('#qssd').val();
			map["jssd"] = jQuery('#jssd').val();
			map["qssj"] = jQuery('#qssj').val();
			map["jssj"] = jQuery('#jssj').val();
			map["jyfs"] = jQuery("input[type='radio']:checked").val();
			map["cdjylx"] = jQuery("#cdjylx").val();
			if(map["jyfs"]==0){
				//获取选择的周次
				var zcd=0;
				$("#selectTR_ZC .selectTH").each(function(){
				  	if($(this).hasClass("ui-selected")){//判断有无此样式
				 			zcd += Math.pow(2,$(this).attr("value")-1);
				 	  	}
			 	});
				map["zcd"]=zcd;
				$("#selectTR_ZC .ui-selected").each(function(i,dom){
					zcList.push($(dom).attr("value"));
					
				});	
				//获取选择的星期
				$("#selectTR_XQJ .ui-selected").each(function(i,dom){
					xqList.push($(dom).attr("value"));
					
				});
				map["xqj"]=xqList.join(",");
			}
			if(map["jyfs"]==0||map["jyfs"]==2){
				//获取选择的节次
				var jcd=0;
				$("#selectTR_JC .selectTH").each(function(){
				  	if($(this).hasClass("ui-selected")){//判断有无此样式
				 			jcd += Math.pow(2,$(this).attr("value")-1);
				 	  	}
			 	});
				map["jcd"]=jcd;

				$("#selectTR_JC .ui-selected").each(function(i,dom){
					jcList.push($(dom).attr("value"));
					
				});	
			}				
			//查询
			search('tabGrid',map);
			//更新右侧浮层数据 			
			if($("#ajxz").prop("checked")){
				$("#jyzc_fc").empty().html("第 "+zcList.join(",")+" 周");
				$("#jyxq_fc").empty().html(xqList.join(","));
				$("#jyjc_fc").empty().html("第 "+jcList.join(",")+" 节");
				$("#jysj_fc").parent().css("display","none");
				$("#jysd_fc").parent().css("display","none");
				$("#jyzc_fc").parent().css("display","block");
				$("#jyxq_fc").parent().parent().css("display","block");	
				$("#jyjc_fc").parent().css("display","block");		
				$("#jyjc_fc").parent().parent().css("display","block");	
			}else{
				$("#jysj_fc").empty().html($("#sjfw").val());//时间范围赋值
				if($("#ajcjy").prop("checked")){
					$("#jyjc_fc").empty().html("第 "+jcList.join(",")+" 节");
					$("#jyzc_fc").parent().css("display","none");
					$("#jyxq_fc").parent().parent().css("display","none");
					$("#jysd_fc").parent().css("display","none");
					$("#jysj_fc").parent().css("display","block");
					$("#jyjc_fc").parent().css("display","block");
					$("#jyjc_fc").parent().parent().css("display","block");
				}
				else if($("#asdjy").prop("checked")){
					$("#jysd_fc").empty().html($("#qssd").val()+"-"+$("#jssd").val());
					$("#jyjc_fc").parent().css("display","none");
					$("#jyzc_fc").parent().css("display","none");
					$("#jyxq_fc").parent().parent().css("display","none");
					$("#jysj_fc").parent().css("display","block");				
					$("#jysd_fc").parent().css("display","block");
					$("#jysd_fc").parent().parent().css("display","block");
				}
				else if($("#asjjy").prop("checked")){
					$("#jyzc_fc").parent().css("display","none");
					$("#jyxq_fc").parent().parent().css("display","none");
					$("#jysd_fc").parent().parent().css("display","none");
					$("#jysj_fc").parent().css("display","block");
				}
			}
			//更新(预约时)需要隐藏提交的数据
			$("#jyfs_fc").val(map["jyfs"]);
			$("#qssj_fc").val(map["qssj"]);
			$("#jssj_fc").val(map["jssj"]);
			$("#qssd_fc").val(map["qssd"]);
			$("#jssd_fc").val(map["jssd"]);
			$("#zcd_fc").val(map["zcd"]);
			$("#xqj_fc").val(map["xqj"]);
			$("#jcd_fc").val(map["jcd"]);
			$("#xqh_id_fc").val(map["xqh_id"]);
			$("#xnm_fc").val(map["xnm"]);
			$("#xqm_fc").val(map["xqm"]);
		}
		//提交前节次选择验证 
		$("#searchForm").validateForm({
			beforeSubmit:function(){
				var selectZcNum= $("#selectTR_ZC .selectTH.ui-selected").length;
				var selectXqNum= $("#selectTR_XQJ .selectTH.ui-selected").length;
				var selectJcNum= $("#selectTR_JC .selectTH.ui-selected").length;
				var jyfs=$('input[name=jyfs]:checked').val();
				if(jyfs==0&&(selectZcNum==0||selectXqNum==0||selectJcNum==0)){
					if(selectZcNum==0){
						$("#selectTR_ZC").parent().addClass("wrapError"); 
						$("#ajxz").tooltips('destroy');
						$("#ajxz").attr({
							"title" 		: 	"请选择预约周次!"
						});
						$("#ajxz").tooltips({
							delay			: 	0,
							html			: 	true,
							placement		:	"top",
							align			:	"left",
							trigger			:   "focus"
						});
						$("#ajxz").tooltips('show');
					}else if(selectXqNum==0){
						$("#selectTR_XQJ").parent().addClass("wrapError"); 
						$("#ajxz").tooltips('destroy');
						$("#ajxz").attr({
							"title" 		: 	"请选择预约星期!"
						});
						$("#ajxz").tooltips({
							delay			: 	0,
							html			: 	true,
							placement		:	"top",
							align			:	"left",
							trigger			:   "focus"
						});
						$("#ajxz").tooltips('show');
					}else if(selectJcNum==0){
						$("#selectTR_JC").parent().addClass("wrapError"); 
						$("#ajxz").tooltips('destroy');
						$("#ajxz").attr({
							"title" 		: 	"请选择预约节次!"
						});
						$("#ajxz").tooltips({
							delay			: 	0,
							html			: 	true,
							placement		:	"top",
							align			:	"left",
							trigger			:   "focus"
						});
						$("#ajxz").tooltips('show');
					}
				}else if(jyfs==2&&selectJcNum==0){
					$("#selectTR_JC").parent().addClass("wrapError"); 
					$("#ajcjy").tooltips('destroy');
					$("#ajcjy").attr({
						"title" 		: 	"请选择预约节次!"
					});
					$("#ajcjy").tooltips({
						delay			: 	0,
						html			: 	true,
						placement		:	"top",
						align			:	"left",
						trigger			:   "focus"
					});
					$("#ajcjy").tooltips('show');
				}else{
					//验证通过 
					$("#ajxz").tooltips('destroy');
					$("#ajcjy").tooltips('destroy');
					$("#selectTR_ZC").parent().removeClass("wrapError");
					$("#selectTR_XQJ").parent().removeClass("wrapError");
					$("#selectTR_JC").parent().removeClass("wrapError"); 
					queryFunc();
				}
				return false;
			}
		});		
		//追加查询验证规则
		$("#qszws,#jszws").blur(function(){
			var min=isNaN(parseInt($("#qszws").val()))?0:parseInt($("#qszws").val());
			var max=isNaN(parseInt($("#jszws").val()))?9999:parseInt($("#jszws").val());
			$("#qszws").rules("remove");	
			$("#jszws").rules("remove");			
			$("#qszws").rules("add",{PositiveInteger:true,max:max});	
			$("#jszws").rules("add",{PositiveInteger:true,min:min});
		});	
		$("#qszws,#jszws").blur();	
		//滚动条
		$(".outer_right_wrapper").mCustomScrollbar({
			autoHideScrollbar:true
		});
		//右侧点击展开和隐藏
		$(".outer").animate({width:"40px"},1000);
		$(".outer_left").click(function(e) {	
			var $width=$(document).width();
            var panelWidth;
            if(($width>440) || ($width==440)){
                panelWidth=440+'px';
            }else{
                panelWidth=$width+'px';
            }
			if($(".outer").css("width")	==	"40px") {
				$(".outer").animate({width:panelWidth},500,function(){
					//更新滚动条
					$(".outer_right_wrapper").css("height",$(".outer_right").actual('height') -  $(".outer_right_footer").actual('height') - 20)
					.mCustomScrollbar("update");
				});
				$(".outer_left .glyphicon").addClass("glyphicon-arrow-right");
			}
			else {
				$(".outer").animate({width:"40px"},500);
				$(".outer_left .glyphicon").removeClass("glyphicon-arrow-right");
			}
		});
		 
		//清空已选教学班
		$("#qkyx").click(function(e){
			e.stopPropagation();
			$("li","ul.list-group").remove();
			countSum();
			$("#tabGrid").resetSelection();
		})
	});		
	</script>

